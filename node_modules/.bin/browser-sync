#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/connoralydon/Documents/compsci_projects/orange-county-tech-events/node_modules/.pnpm/browser-sync@3.0.2/node_modules/browser-sync/dist/node_modules:/Users/connoralydon/Documents/compsci_projects/orange-county-tech-events/node_modules/.pnpm/browser-sync@3.0.2/node_modules/browser-sync/node_modules:/Users/connoralydon/Documents/compsci_projects/orange-county-tech-events/node_modules/.pnpm/browser-sync@3.0.2/node_modules:/Users/connoralydon/Documents/compsci_projects/orange-county-tech-events/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/connoralydon/Documents/compsci_projects/orange-county-tech-events/node_modules/.pnpm/browser-sync@3.0.2/node_modules/browser-sync/dist/node_modules:/Users/connoralydon/Documents/compsci_projects/orange-county-tech-events/node_modules/.pnpm/browser-sync@3.0.2/node_modules/browser-sync/node_modules:/Users/connoralydon/Documents/compsci_projects/orange-county-tech-events/node_modules/.pnpm/browser-sync@3.0.2/node_modules:/Users/connoralydon/Documents/compsci_projects/orange-county-tech-events/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../browser-sync/dist/bin.js" "$@"
else
  exec node  "$basedir/../browser-sync/dist/bin.js" "$@"
fi
